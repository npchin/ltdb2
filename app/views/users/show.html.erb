<div class="page-header">
  <h1>User #<%= @user.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>First name</dt>
  <dd><%= @user.first_name %></dd>

  <dt>Last name</dt>
  <dd><%= @user.last_name %></dd>

  <dt>Email</dt>
  <dd><%= ChronicDuration::output(75, :format => :chrono)%></dd>

  <!-- <dd><%= @user.email %></dd> -->

  <dt>Screen name</dt>
  <dd><%= @user.screen_name %></dd>

  <dt>Zip</dt>
  <dd><%= ChronicDuration::output(@user.zip , :format => :chrono)%></dd>

</dl>
